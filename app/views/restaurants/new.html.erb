<div class="container center">
  <h1>New Restaurant</h1>
  <div class="border">
    <%= simple_form_for @restaurant do |f| %>
      <%= f.input :name %>
      <%= f.input :category %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.submit "Add Restaurant", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>